{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended", "tslint-eslint-rules"],
  "jsRules": {},
  "rules": {
    "arrow-parens": [true, "ban-single-arg-parens"],
    "no-console": false,
    "prefer-const": [
      true,
      {
        "destructuring": "all"
      }
    ],
    "quotemark": [true, "single"]
  },
  "rulesDirectory": []
}
